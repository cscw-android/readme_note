<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.readmenote"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_web01"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.readmenote.MyNoteActivity"
            android:label="@string/title_activity_my_note" >
        </activity>
        <activity
            android:name="com.example.readmenote.NewNoteActivity"
            android:label="@string/title_activity_new_note" >
        </activity>
        <activity android:name="com.example.readmenote.AddNote_moodtagging" >
        </activity>
        <activity android:name="com.example.readmenote.detail" >
        </activity>
        <activity
            android:name="com.example.readmenote.ShareActivity"
            android:label="@string/title_activity_share" >
        </activity>
        <activity
            android:name="com.example.readmenote.ThingDetail"
            android:label="@string/title_activity_share" >
        </activity>
    </application>

</manifest>